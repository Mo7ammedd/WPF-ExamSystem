<Window x:Class="Exam_system.commands"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="commands" Height="450" Width="800" Background="#04c582">
    <Grid>
        <Border Background="#2e3137" BorderThickness="1" CornerRadius="20" Margin="20">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Margin="20">
                <TextBlock Text="Welcome to the Commands Window!" FontSize="20" Foreground="White" Margin="0,0,0,20"/>
                <TextBox x:Name="txtParameter" Width="200" Height="30" Background="#545d6a" Foreground="White" FontSize="18" Margin="0,0,0,10"/>
                <Button Content="Get Instructors in All Intakes" Click="btnGetInstructors_Click" Background="#545d6a" Foreground="White" FontSize="18" Margin="0,0,0,10"/>
                <Button Content="Show Questions In Course" Click="btnCommand1_Click" Background="#545d6a" Foreground="White" FontSize="18" Margin="0,0,0,10"/>
<ListBox x:Name="listBoxCommands" Width="400" Height="200" Background="#545d6a" Foreground="White" FontSize="16" Margin="0,0,0,10"/>
                <Button Content="Back to Login" Click="btnBackToLogin_Click" Background="#545d6a" Foreground="White" FontSize="18" Margin="0,0,0,10"/>
            </StackPanel>
        </Border>
    </Grid>
</Window>